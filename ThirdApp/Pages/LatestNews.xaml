<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ThirdApp.Pages.LatestNews">
    <ContentView.Content>
        <StackLayout>
            <Label x:Name="title" FontSize="25" Padding="5,10" TextColor="Goldenrod"/>
            <Label x:Name="subtitle" FontSize="20" Padding="5,10" TextColor="DarkOliveGreen"/>
            <Label x:Name="desc" FontSize="16" BackgroundColor="Beige" Padding="5,10"/>
        </StackLayout>
    </ContentView.Content>
</ContentView>